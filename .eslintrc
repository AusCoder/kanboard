{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      // Allow
      "arrow-body-style": 0,
      // import
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "max-len": 0,
      "no-underscore-dangle": 0,
      # // React
       "react/forbid-prop-types": 0,
  #     "react/self-closing-comp": 0,
      # "react/jsx-filename-extension": 0
    }
}
